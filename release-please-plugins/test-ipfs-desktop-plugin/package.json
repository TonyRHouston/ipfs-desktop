{
  "name": "release-please-test-ipfs-desktop-plugin",
  "version": "0.0.0",
  "description": "This package is not meant to be published to npm.",
  "private": true,
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "build": "aegir build"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./errors": {
      "types": "./dist/src/errors.d.ts",
      "import": "./dist/src/errors.js"
    }
  },
  "browser": {
    "fs": false,
    "path": false,
    "url": false
  },
  "devDependencies": {
    "aegir": "^41.0.4",
    "release-please": "^16.0.0"
  }
}
